{
  "manifest_version": 3,
  "name": "Stack Overflow Mentor",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "http://localhost:3000/*"
  ],  "host_permissions": ["*://*.stackoverflow.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["*://*.stackoverflow.com/*"],
    "js": ["content-script.js"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon.png",
      "32": "icon/icon.png",
      "48": "icon/icon.png",
      "128": "icon/icon.png"
    }
  },
  "icons": {
    "16": "icon/icon.png",
    "32": "icon/icon.png",
    "48": "icon/icon.png",
    "128": "icon/icon.png"
  }
}