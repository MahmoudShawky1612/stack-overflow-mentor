{
  "manifest_version": 3,
  "name": "Stack Overflow Mentor",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "http://localhost:3000/*"
  ],  "host_permissions": ["*://*.stackoverflow.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["*://*.stackoverflow.com/*"],
    "js": ["content-script.js"]
  }],
  "action": {
    "default_popup": "popup.html" 
  } 
}